@model GetByIdContactDto
@{
    ViewData["Title"] = "GetByIdContact";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">
                    <form method="post">
                        <input type="hidden" asp-for="ContactId" />
                        <label></label>
                        <input type="text" class="form-control" asp-for="ContactNameSurname" />
                        <br />
                        <label></label>
                        <input type="text" class="form-control" asp-for="ContactEmail" />
                        <br />
                        <label></label>
                        <input type="text" class="form-control" asp-for="ContactSendDate" />
                        <br />
                        <label></label>
                        <input type="text" class="form-control" asp-for="ContactIsRead" />
                        <br />
                        <label></label>
                        <input type="text" class="form-control" asp-for="ContactSubject" />
                        <br />
                        <label></label>
                        <textarea class="form-control" asp-for="ContactMessage" rows="5"></textarea>
                        <br />
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>